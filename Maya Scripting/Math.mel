global float $result;

global proc float Add(float $numbers[])
{
    float $sum = 0;
    for($i=0; $i < size($numbers); $i++)
    {
        $sum += $numbers[$i];
    }
    
    return $sum;
}

global proc float Subtract(float $numbers[])
{
    float $sum = 0;
    for($i=0; $i < size($numbers); $i++)
    {
        $sum += $numbers[$i];
    }
    
    return $sum;
}

